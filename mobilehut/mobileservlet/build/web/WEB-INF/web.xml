<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>compare</servlet-name>
        <servlet-class>compare</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sell</servlet-name>
        <servlet-class>sell</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>compare0</servlet-name>
        <servlet-class>comparemobile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>compareearphone</servlet-name>
        <servlet-class>compareearphone</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>comparespeaker</servlet-name>
        <servlet-class>comparespeaker</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>comparemobile</servlet-name>
        <servlet-class>comparemobile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sell_mobile</servlet-name>
        <servlet-class>sell_mobile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sell_earphone</servlet-name>
        <servlet-class>sell_earphone</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sell_speaker</servlet-name>
        <servlet-class>sell_speaker</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>compare</servlet-name>
        <url-pattern>/compare</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sell</servlet-name>
        <url-pattern>/sell</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>compare0</servlet-name>
        <url-pattern>/compare0</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>compareearphone</servlet-name>
        <url-pattern>/compareearphone</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>comparespeaker</servlet-name>
        <url-pattern>/comparespeaker</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>comparemobile</servlet-name>
        <url-pattern>/comparemobile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sell_mobile</servlet-name>
        <url-pattern>/sell_mobile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sell_earphone</servlet-name>
        <url-pattern>/sell_earphone</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sell_speaker</servlet-name>
        <url-pattern>/sell_speaker</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
